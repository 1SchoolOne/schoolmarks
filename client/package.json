{
	"name": "schoolmarks-client",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"packageManager": "yarn@4.6.0",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b ./tsconfig.build.json && vite build",
		"lint": "eslint ./src",
		"lint:fix": "eslint --fix ./src",
		"lint:report": "eslint --output-file eslint_report.json --format json ./src",
		"format": "prettier ./src --check",
		"format:fix": "prettier ./src --write",
		"test": "vitest run",
		"test:watch": "vitest watch",
		"preview": "vite preview",
		"generate-api-client": "openapi-generator-cli generate -i ../api/openapi-schema.yml -g typescript-axios -o ./src/api-client",
		"postinstall": "cd .. && husky client/.husky",
		"knip": "knip"
	},
	"dependencies": {
		"@1schoolone/ui": "1.1.0",
		"@openapitools/openapi-generator-cli": "2.16.3",
		"@tanstack/react-query": "5.59.0",
		"antd": "5.24.2",
		"axios": "1.8.4",
		"classnames": "2.5.1",
		"dayjs": "1.11.13",
		"lucide-react": "0.447.0",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"react-router-dom": "6.27.0",
		"store2": "2.14.4"
	},
	"devDependencies": {
		"@eslint/js": "9.13.0",
		"@testing-library/dom": "10.4.0",
		"@testing-library/react": "16.1.0",
		"@trivago/prettier-plugin-sort-imports": "4.3.0",
		"@types/node": "22.8.1",
		"@types/react": "18.3.1",
		"@types/react-dom": "18.3.1",
		"@vitejs/plugin-react-swc": "3.8.0",
		"@vitest/ui": "3.0.6",
		"eslint": "9.13.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-jsdoc": "50.3.1",
		"eslint-plugin-prettier": "5.2.1",
		"eslint-plugin-react-hooks": "5.0.0",
		"eslint-plugin-react-refresh": "0.4.13",
		"eslint-plugin-testing-library": "7.1.1",
		"globals": "15.11.0",
		"husky": "9.1.7",
		"jsdom": "25.0.1",
		"knip": "5.52.0",
		"less": "4.2.0",
		"msw": "2.7.0",
		"openapi-typescript": "7.4.3",
		"prettier": "3.3.3",
		"prettier-plugin-jsdoc": "1.3.0",
		"typescript": "5.6.2",
		"typescript-eslint": "8.10.0",
		"vite": "6.1.1",
		"vitest": "3.0.6"
	}
}
